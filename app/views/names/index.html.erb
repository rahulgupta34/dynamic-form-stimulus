<p style="color: green"><%= notice %></p>

<h1>Names</h1>
<%= turbo_stream_from "names" %>
<%= turbo_frame_tag "names" do  %>
<div id="names">
  <% @names.each do |name| %>
    <%= render name %>
    <p>
      <%= link_to "Show this name", name %>
    </p>
  <% end %>
</div>
<% end %>
<%= link_to "New name", new_name_path %>
